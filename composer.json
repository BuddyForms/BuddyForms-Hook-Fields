{
  "name": "themekraft/buddyforms-hook-fields",
  "description": "Have full control of your fields output. Hook your BuddyForms Form Fields via options and display them everywhere on your site.",
  "type": "wordpress-plugin",
  "keywords": [
    "form builder",
    "contact forms",
    "post forms",
    "user-generated content",
    "custom post types",
    "user submitted posts",
    "contact form",
    "form",
    "forms",
    "frontend post"
  ],
  "homepage": "https://themekraft.com/buddyforms/",
  "license": "GPLv2 or later",
  "minimum-stability": "dev",
  "require-dev": {
    "consolidation/robo": "^1.0.0",
    "bamarni/composer-bin-plugin": "dev-master",
    "phpunit/phpunit": "^6.1",
    "php-coveralls/php-coveralls": "^2.0@dev",
    "squizlabs/php_codesniffer": "3.*",
    "friendsofphp/php-cs-fixer": "^2.9",
    "jakub-onderka/php-parallel-lint": "dev-master",
    "jakub-onderka/php-console-highlighter": "dev-master",
    "wp-coding-standards/wpcs": "dev-master",
    "symplify/easy-coding-standard": "^5.2@dev",
    "brainmaestro/composer-git-hooks": "dev-master"
  },
  "scripts": {
    "package": "sh .tk/package.sh buddyforms-hook-fields",
    "wp-release": "sh .tk/wp_release.sh",
    "beta-release": "sh .tk/full-deploy.sh buddyforms-hook-fields 412 false beta",
    "full-release": "sh .tk/full-deploy.sh buddyforms-hook-fields 412 false released",
    "update-freemius": "sh .tk/update-freemius.sh ../wordpress-sdk includes/resources/freemius"
  }
}
